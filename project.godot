; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PocketStation"
run/main_scene="res://Main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

Controller="*res://globals/Controller.gd"

[debug]

gdscript/warnings/untyped_declaration=1

[display]

window/size/viewport_width=640
window/size/viewport_height=1387
window/size/mode=2
window/stretch/mode="viewport"
window/handheld/orientation=1

[editor]

naming/scene_name_casing=1
naming/script_name_casing=1

[gui]

theme/custom="res://project_theme.tres"

[physics]

common/physics_ticks_per_second=100

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
environment/defaults/default_clear_color=Color(0.393086, 0.393086, 0.393085, 1)
